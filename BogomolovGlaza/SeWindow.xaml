<Window x:Class="BogomolovGlaza.SeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BogomolovGlaza"
        mc:Ignorable="d"
        Title="SetWindow" Height="200" Width="300">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <TextBlock Text="Изменить приоритет на:" FontSize="18"/>
            <TextBox Width="150" Name="TBPriority"/>
            <Button Content="Сохранить" Height="20" FontSize="12" Width="90" Name="SetBtn" Click="SetBtn_Click"/>
        </StackPanel>
    </Grid>
</Window>
